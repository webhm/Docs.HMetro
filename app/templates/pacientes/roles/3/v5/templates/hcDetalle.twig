<div class="row col-12">
    <div class="d-flex wd-100p align-items-center justify-content-between mg-b-30">
        <span class="tx-12 mg-l-100">
            Página:
            <span class="page_numHC"></span>
            /
            <span class="page_countHC"></span>
        </span>
        <div class="d-flex flex-row-reverse">

            <span class="reset-detalle-hc mg-l-15 pd-1 bd bd-0" data-toggle="tooltip" title="Cerrar" style="cursor:pointer;">
                <i class="fas fa-times-circle tx-16 lh--9 tx-gray-500"></i>
            </span>
            <a class="nextHC pd-1 bd bd-0" href="#" data-toggle="tooltip" title="Siguiente">
                <i class="fas fa-chevron-circle-right tx-15 lh--9 tx-primary"></i>
            </a>
            <a class="prevHC pd-1 bd bd-0 mg-r-15" href="#" data-toggle="tooltip" title="Anterior">
                <i class="fas fa-chevron-circle-left tx-15 lh--9 tx-primary"></i>
            </a>
            <a class="pd-1 bd bd-0 mg-r-15" href="%pdfUrl%" target="_blank" data-toggle="tooltip" title="Descargar" download="download">
                <i class="fas fa-download tx-15 lh--9 tx-primary"></i>
            </a>
            <a class="contraerHC pd-1 bd bd-0 mg-r-15 " href="#" data-toggle="tooltip" title="" data-original-title="Contraer" aria-describedby="tooltip859075">
                <i class="fas fa-search-minus tx-15 lh--9 tx-primary"></i>
            </a>
            <a class="expandirHC pd-1 bd bd-0 mg-r-15 " href="#" data-toggle="tooltip" title="" data-original-title="Aumentar" aria-describedby="tooltip859075">
                <i class="fas fa-search-plus tx-15 lh--9 tx-primary"></i>
            </a>
            <a class="compartirHC pd-1 bd bd-0 mg-r-15 d-none" href="#" data-toggle="tooltip" title="" data-original-title="Compartir Historia Clínica" aria-describedby="tooltip859075">
